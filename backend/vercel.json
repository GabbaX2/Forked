{
  "version": 2,
  "builds": [
    {
      "src": "index.js",  // o il tuo file di ingresso principale
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js",  // deve corrispondere al file sopra
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
